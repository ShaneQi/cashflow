<form theme="dark">
  <init>
    <eval token="cycle">strftime(now() - 3600 * 24 * 30, "%y%m")</eval>
    <set token="inOut">OUT</set>
  </init>
  <label>Budget</label>
  <fieldset submitButton="false">
    <input type="text" token="year" searchWhenChanged="true">
      <label>Year</label>
      <default>24</default>
    </input>
  </fieldset>
